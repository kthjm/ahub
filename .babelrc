{
  "presets": [
    "react"
  ],
  "env": {
    "ENV": {
      "presets": [
        "env"
      ]
    },
    "TEST": {
       "presets": [
         "env",
         "power-assert"
       ],
       "plugins": [
         "istanbul"
       ]
    },
    "BUILD": {
      "presets": [
        ["env", { "targets": { "node": "8.11.3" }, "modules": false }]
      ],
      "plugins": [
        "external-helpers"
      ]
    }
  }
}