  "license": "MIT",
  "main": "dist/index.js",
  "bin": "bin/tuft.js",
  "description": "",
  "keywords": [],
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "tuft": "cross-env BABEL_ENV=BIN node -r babel-register src/bin.index.js",
    "tuft:bin": "node bin/tuft.js",
    "test": "cross-env BABEL_ENV=TEST nyc mocha --no-timeouts",
    "start": "chin watch -c",
    "build:roll": "cross-env BABEL_ENV=BUILD rollup -c",
    "build:chin": "chin -c",
    "build": "run-s build:roll build:chin"
  },
  "dependencies": {
    "chin": "https://github.com/chinjs/chin#3.1.2",
    "commander": "^2.16.0",
    "favicons": "^5.1.1",
    "fs-extra": "^7.0.0",
    "html-tag": "^2.0.0",
    "pretty": "^2.0.0",
    "sitemap": "^1.13.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-power-assert": "^2.0.0",
    "browser-sync": "^2.24.6",
    "codecov": "^3.0.4",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "mock-fs": "^4.5.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^12.0.2",
    "power-assert": "^1.6.0",
    "rollup": "^0.63.4",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-prettier": "^0.4.0",
    "serve": "^9.4.0",
    "stylus": "^0.54.5"
  }
}
